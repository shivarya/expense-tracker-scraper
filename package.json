{
  "name": "expense-tracker-scraper",
  "version": "1.0.0",
  "description": "Automated data scraper for stocks, mutual funds, and SMS transactions",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "auth": "tsx src/auth/token.ts auth",
    "auth:force": "tsx src/auth/token.ts auth --force",
    "auth:gmail": "tsx src/auth/gmail-auth.ts",
    "fetch:all": "npm run fetch:stocks && npm run fetch:mf && npm run fetch:cc",
    "fetch:stocks": "npm run fetch:stocks:zerodha && npm run fetch:stocks:groww",
    "fetch:stocks:zerodha": "tsx src/stocks/zerodha.ts",
    "fetch:stocks:groww": "tsx src/stocks/groww.ts",
    "fetch:mf": "npm run fetch:mf:cams",
    "fetch:mf:cams": "tsx src/mutual-funds/cams.ts",
    "cdsl:download": "tsx src/investments/download-latest-cdsl.ts",
    "cdsl:extract": "tsx src/investments/extract-cdsl.ts",
    "cdsl:enrich": "tsx src/investments/enrich-cdsl.ts",
    "nps:check-mail": "tsx src/long-term/check-nps-mail.ts",
    "nps:download": "tsx src/long-term/download-latest-nps.ts",
    "nps:extract": "tsx src/long-term/extract-nps.ts",
    "nps:enrich": "tsx src/long-term/enrich-nps.ts",
    "sync:nps": "tsx src/long-term/sync-nps.ts",
    "sync:long-term": "npm run nps:check-mail && npm run nps:download && npm run nps:extract && npm run nps:enrich && npm run sync:nps",
    "pf:setup": "tsx src/long-term/epfo-passbook.ts --setup",
    "pf:setup:visible": "tsx src/long-term/epfo-passbook.ts --setup --show-password",
    "pf:fetch": "tsx src/long-term/epfo-passbook.ts --fetch",
    "pf:run": "npm run pf:setup && npm run pf:fetch",
    "pf:enrich": "tsx src/long-term/enrich-pf.ts",
    "pf:forget-creds": "tsx src/long-term/epfo-passbook.ts --forget-creds",
    "sync:pf": "tsx src/long-term/sync-pf.ts",
    "mfc:setup": "tsx src/long-term/mfcentral-statement.ts --setup",
    "mfc:setup:visible": "tsx src/long-term/mfcentral-statement.ts --setup --show-password",
    "mfc:fetch": "tsx src/long-term/mfcentral-statement.ts --fetch",
    "mfc:run": "npm run mfc:setup && npm run mfc:fetch",
    "mfc:forget-creds": "tsx src/long-term/mfcentral-statement.ts --forget-creds",
    "mfc:extract": "tsx src/long-term/extract-mfcentral.ts",
    "mfc:enrich": "tsx src/long-term/enrich-mfcentral.ts",
    "sync:mf": "tsx src/long-term/sync-mfcentral.ts",
    "sync:mf:full": "npm run mfc:run && npm run mfc:extract && npm run mfc:enrich && npm run sync:mf",
    "fetch:cc:raw": "tsx src/transactions/extract-raw.ts",
    "fetch:cc:raw:all": "tsx src/transactions/extract-raw.ts --all",
    "fetch:cc:3m": "tsx src/transactions/extract-raw.ts --all --time=3m",
    "fetch:cc:6m": "tsx src/transactions/extract-raw.ts --all --time=6m",
    "fetch:cc:1y": "tsx src/transactions/extract-raw.ts --all --time=1y",
    "fetch:cc:2y": "tsx src/transactions/extract-raw.ts --all --time=2y",
    "fetch:cc:all-time": "tsx src/transactions/extract-raw.ts --all --time=all",
    "extract:cc:pdf-parse": "tsx src/transactions/extract-with-pdf-parse.ts",
    "extract:cc:emis": "tsx src/transactions/extract-emis.ts",
    "enrich:cc": "tsx src/transactions/enrich-and-categorize.ts",
    "enrich:cc:emis": "tsx src/transactions/enrich-emis.ts",
    "preview:duplicates": "tsx src/transactions/preview-duplicates.ts --type=all",
    "preview:duplicates:cc": "tsx src/transactions/preview-duplicates.ts --type=transactions",
    "preview:duplicates:emis": "tsx src/transactions/preview-duplicates.ts --type=emis",
    "preview:duplicates:stocks": "tsx src/transactions/preview-duplicates.ts --type=stocks",
    "preview:duplicates:mf": "tsx src/transactions/preview-duplicates.ts --type=mutual_funds",
    "analyze:cc": "node scripts/compute-summary.mjs",
    "merchant:report": "node scripts/generate-merchant-report.mjs",
    "merchant:suggest": "node scripts/suggest-merchant-aliases.mjs",
    "sync:cc:enriched": "tsx src/transactions/sync-enriched.ts",
    "sync:cc:emis": "tsx src/transactions/sync-emis.ts",
    "test:azure": "tsx src/test-azure.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.21.0",
    "axios": "^1.7.0",
    "date-fns": "^3.0.0",
    "dotenv": "^16.4.0",
    "googleapis": "^134.0.0",
    "markitdown": "^0.0.4",
    "node-qpdf2": "^6.0.0",
    "open": "^10.0.0",
    "openai": "^4.28.0",
    "pdf-parse": "^2.4.5",
    "playwright": "^1.42.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.0"
  },
  "keywords": [
    "scraper",
    "automation",
    "finance",
    "sms-parser"
  ],
  "author": "",
  "license": "MIT"
}
